{
  "name": "@shopify/app-extensions-playground",
  "version": "0.0.9",
  "private": true,
  "dependencies": {
    "@shopify/app-extensions-polaris-components": "^0.0.9",
    "@shopify/app-extensions-renderer": "^0.0.9",
    "@shopify/app-extensions-ui-builder": "^0.0.9",
    "@shopify/polaris": "^3.12.0",
    "@shopify/react-testing": "^1.6.0",
    "@svgr/webpack": "4.1.0",
    "@types/styled-components": "4.1.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chart.js": "^2.8.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "hookrouter": "^1.1.6",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "immutable": "^4.0.0-rc.12",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-cssobj": "1.2.5",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-simple-code-editor": "^0.9.10",
    "react-syntax-highlighter": "^10.3.0",
    "resolve": "1.10.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test-jest": "jest",
    "check": "tsc --pretty --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chart.js": "^2.7.53",
    "@types/hookrouter": "^2.2.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/prismjs": "^1.16.0",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-shopify": "^31.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.8.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.18.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "tslint": "^5.16.0",
    "tslint-lines-between-class-members": "^1.3.1",
    "typescript": "^3.4.3",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "jest": {
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/@shopify/react-testing/matchers"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "^.+\\.module\\.(css|sass|scss)$",
      "<rootDir>/node_modules/?!(refractor|flexboxgrid2).+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/src/tests/shared-tests"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
