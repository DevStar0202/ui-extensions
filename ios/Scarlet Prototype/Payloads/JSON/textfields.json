[
    {
        "type": "textfield",
        "properties": {
            "label": "Label One",
            "onChange": {
                "type": "lambda",
                "arg": "$value",
                "action": {
                    "type": "set",
                    "key": "tf1",
                    "value": "pre $value"
                }
            }  
        }
    },
    {
        "type": "textfield",
        "properties": {
            "label": "Label Two",
            "onChange": {
                "type": "lambda",
                "arg": "$value",
                "action": {
                    "type": "set",
                    "key": "tf2",
                    "value": "$value post"
                }
            }
        }
    },
    {
        "type": "textfield",
        "properties": {
            "label": "Label Three",
            "onChange": {
                "type": "lambda",
                "arg": "$value",
                "action": {
                    "type": "alert",
                    "properties": {
                        "message": "textfield contents: $value"
                    }
                }
            }
        }
    }
]
